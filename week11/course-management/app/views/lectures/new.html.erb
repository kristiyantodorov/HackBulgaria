<h1>Create lecture</h1>

<%= form_for :lecture, url: lectures_path do |f| %>
  <div.class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
  </div>
  <div.class="field">
    <%= f.label :text %><br>
    <%= f.text_area :text %><br>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
